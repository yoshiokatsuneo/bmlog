<h1>Edit tag</h1>

<form id="edit-tag" name="tag">
  <div class="field">
    <label for="name"> name:</label>
    <input type="text" name="name" id="name" value="<%= name %>" >
  </div>

  <div class="field">
    <label for="bookmark"> bookmark:</label>
    <input type="text" name="bookmark" id="bookmark" value="<%= bookmark %>" >
  </div>

  <div class="actions">
    <input type="submit" value="Update Tag" />
  </div>

</form>

